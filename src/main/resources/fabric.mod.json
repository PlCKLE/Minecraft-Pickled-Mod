{
	"schemaVersion": 1,
	"id": "pickled",
	"version": "${version}",
	"name": "Minecraft: Pickled",
	"description": "This is a mod that tries to improve Minecraft while not straying from the direction Mojang has been going.",
	"authors": [
		"Pickle"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/pickled/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.picklemod.Pickled"
		],
		"fabric-datagen": [
			"net.picklemod.PickledDataGenerator"
		],
      "client": [
          "net.picklemod.PickledModClient"
      ]
	},
	"mixins": [
		"pickled.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.8",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}